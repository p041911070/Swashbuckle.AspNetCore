{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "SwaggerGen": {
    "SwaggerGeneratorOptions": {
      "SwaggerDocs": {
        "v1": {
          "Title": "V1 API Docs",
          "Version": "v1"
        }
      }
    }
  },
  "SwaggerUI": {
    "RoutePrefix": "swagger",
    "ConfigObject": {
      "Urls": [ { "Url": "/swagger/v1/swagger.json", "Name": "V1 Docs" } ],
      "DeepLinking": true,
      "DisplayOperationId": true,
      "DefaultModelsExpandDepth": -1,
      "DefaultModelExpandDepth": 2,
      "DefaultModelRendering": "model",
      "DocExpansion": "full",
      "Filter": "",
      "ShowExtensions": true
    }
  },
  "ReDoc": {
    "RoutePrefix": "redoc",
    "SpecUrl": "/swagger/v1/swagger.json",
    "ConfigObject": {
      "HideDownloadButton": false,
      "ExpandResponses": "",
      "PathInMiddlePanel": true,
      "HideLoading": true,
      "NativeScrollbars": true
    }
  }
}